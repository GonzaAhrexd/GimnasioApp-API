model Usuario {
  id_usuario    Int     @id @default(autoincrement())
  nombre  String?
  email String  @unique
  rol  String  
  pass String?
  cambiarPass Boolean @default(true)
}

model Cliente {
  id_cliente    Int     @id @default(autoincrement())
  id_usuario  Int     @unique
  objetivo  String?
  profesor_asignado  Int?
}

model Profesor {
  id_profesor    Int     @id @default(autoincrement())
  id_usuario  Int     @unique
}

